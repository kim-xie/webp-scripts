{
  "name": "webp-scripts",
  "version": "1.0.4",
  "description": "webp script",
  "main": "./dist/bin/webp-scripts.js",
  "types": "./dist/bin/webp-scripts.d.ts",
  "bin": {
    "webp-scripts": "./dist/bin/webp-scripts.js"
  },
  "scripts": {
    "setup": "rm -rf node_modules && npm install --no-package-lock",
    "start": "rollup -c rollup.config.js -w",
    "build:rollup": "rm -fr dist && rollup -c rollup.config.js",
    "build": "rm -fr dist && tsc && npm run copy",
    "copy": "mv dist/src/* dist/ && rm -fr dist/src",
    "test": "jest --coverage --verbose -u",
    "lint": "eslint --ext .ts --format=pretty src/**",
    "lint:fix": "eslint --fix --ext .ts --format=pretty src/**",
    "prettier": "prettier -c -w src/**",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "preCommit": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "npm run prettier",
      "npm run lint:fix"
    ]
  },
  "keywords": [
    "webp",
    "scripts"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "author": "kim",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mr-kings/webp-scripts.git"
  },
  "homepage": "https://github.com/mr-kings/webp-scripts#readme",
  "bugs": {
    "url": "https://github.com/mr-kings/webp-scripts/issues"
  },
  "engines": {
    "node": "14.17.6"
  },
  "sideEffects": "false",
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "commander": "^8.2.0",
    "cp": "^0.2.0",
    "semver": "^7.3.5",
    "webp-converter": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/conventional-changelog": "^3.1.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "husky": "^7.0.0",
    "jest": "^27.1.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
